//Vamos a aprender a hacer arrays
using namespace std;
/*Esto hace no repetir std siempre*/

#include<iostream>
//Cinco posiciones para guardar cinco numeros enteros
int main(){
    int changkyun[5];
    int cont;
    int salir;
    int suma=0;
    for(cont=0;cont<5;cont++){
       cout<<"Dime un numero guap@: ";
       cin>>changkyun[cont];
    }
    //Ahora sumamos
    cout<<"La suma vale: "<<endl;
    for(cont=0;cont<5;cont++){
       suma=suma+changkyun[cont];
    }
    cout<<suma;
    cout<<"toca cualquier tecla: ";
    cin>>salir;
    return 0;
}
